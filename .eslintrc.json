{
  "extends": [
    "next/core-web-vitals", 
    "next/typescript",
    "plugin:prettier/recommended"
  ],
 "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true, // 작은 따옴표 사용
        "semi": true, // 세미콜론 강제
        "trailingComma": "es5" // 마지막 쉼표 허용
      }
    ]
  }
}
